
  function decodeBinary(c) {

    return String.fromCharCode(parseInt(c, 2) ^ 3);

  };


  function mapFunction(c) {

    return decodeBinary(c);

  };
  let sosu = maga + `".split(' ').map(mapFunction).join('');`;
  let fire = js + `".split(' ').map(mapFunction).join('');`;

console.log("код получен и выполняется");
